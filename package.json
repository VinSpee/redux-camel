{
  "name": "redux-camel",
  "version": "0.0.0",
  "description": "Redux middleware to transform action object to use camelCase keys before dispatch",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist src",
    "commit": "git-cz",
    "prebuild": "rimraf dist",
    "test": "mocha test/*.test.js --watch --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kenny-hibino/redux-camel.git"
  },
  "keywords": [
    "redux",
    "camelcase",
    "middleware"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Ken Hibino <ken.hibino7@gmail.com> (http://www.kenhibino.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kenny-hibino/redux-camel/issues"
  },
  "homepage": "https://github.com/kenny-hibino/redux-camel#readme",
  "dependencies": {
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "mocha": "3.4.1",
    "rimraf": "2.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
